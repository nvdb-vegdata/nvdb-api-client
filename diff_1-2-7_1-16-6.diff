+                GsonUtil.parseRoadSysRefMember(obj, "vegsystemreferanse"),
+                RefLinkPartType.fromValue(parseStringMember(obj,"type")),
+                parseContractAreas(obj),
+                parseRoutes(obj),
+                parseStreet(obj));
