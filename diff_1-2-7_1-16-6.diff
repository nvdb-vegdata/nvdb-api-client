+            return RouteParser.parseBrief(result);
+        } else {
+            return RouteParser.parseDetailed(result);
+        }
+    }
+
+    public RouteOnRoadNet postRouteOnRoadnet(RoadNetRouteRequest request) {
+        WebTarget target = getWebTarget();
